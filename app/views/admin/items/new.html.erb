<main>
  <h3>商品新規登録</h3>
  <%= form_with model: @item, url: admin_items_path do |f| %>
    <h4>画像</h4>
    <%= f.file_field :image, accept: "image/*" %>
    <h4>商品名</h4>
    <%= f.text_field :name %>
    <h4>商品紹介</h4>
    <%= f.text_area :introduction %>
    <h4>価格</h4>
    <%= f.text_field :price %>
    <%= f.submit "新規登録" %>
  <% end %>
</main>